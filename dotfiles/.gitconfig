[user]
	name = Gregory Bataille
	email = gbataille.dev@gmail.com
[alias]
	st = status
	co = checkout
	cob = checkout -b
	p = push
	b = branch -v
	t = tag -a
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[github]
	user = gbataille
[color]
	ui = true
